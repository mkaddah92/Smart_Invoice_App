{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Smart_Invoice_App",
      "runtimeExecutable": "cds",
      "request": "launch",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "PreLaunchTask for Run Smart_Invoice_App",
      "envFile": "${workspaceFolder}/env/.env1",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/Smart_Invoice_App\\\",\\\"profileName\\\":\\\"\\\"}\"}"
      }
    },
    {
      "name": "cds serve hybrid",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch",
        "--profile",
        "hybrid"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}